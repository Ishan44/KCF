<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css"
        integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/index.css">
    <title>Document</title>
</head>

<body>

    <%- include("partials/navbar"); -%>

    <div class="flex-container">
        <div class="sensor-table">
            <div class="table-title">
                <h3>Miner Details</h3>
            </div>
            <table class="table-fill">
                <thead>
                    <tr>
                        <th class="text-left">Miner ID</th>
                        <th class="text-left">Miner Name</th>
                        <th class="text-left">Age </th>
                        <th class="text-left">Address</th>
                        <th class="text-left">Contact No</th>
                        <th class="text-left">Emergency Contact No</th>
                        <th class="text-left">Realtionship</th>

                    </tr>
                </thead>
                <tbody class="table-hover">
                    <% found.forEach(function(found){ %>
                    <tr>
                        <td><%=found.mid %></td>
                        <td><%=found.name %></td>
                        <td><%=found.age %></td>
                        <td><%=found.address %></td>
                        <td><%=found.contact %></td>
                        <td><%=found.econtact %></td>
                        <td><%=found.relationship %></td>
                    </tr>
                    <%})%>
                </tbody>
            </table>
        </div>
    </div>


    <%- include("partials/navbarfooter"); -%>

</body>

</html>